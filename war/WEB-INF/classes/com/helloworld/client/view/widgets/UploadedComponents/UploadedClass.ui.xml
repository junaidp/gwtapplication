<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<div style="padding:5px">
			<g:HorizontalPanel>
				<g:Label text="Id" width="80px" />
				<g:ListBox visibleItemCount="1" ui:field="listIds" name="id" />
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Label text="Name" width="80px" />
				<g:TextBox ui:field="textBoxName" name="name" />
			</g:HorizontalPanel>

			<g:HorizontalPanel>
				<g:Label text="User_Name" width="80px" />
				<g:TextBox ui:field="textBoxUser_Name" name="user_name" />
			</g:HorizontalPanel>

			<g:HorizontalPanel>
				<g:Label text="Password" width="81px" />
				<g:PasswordTextBox width="162px" />
			</g:HorizontalPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 