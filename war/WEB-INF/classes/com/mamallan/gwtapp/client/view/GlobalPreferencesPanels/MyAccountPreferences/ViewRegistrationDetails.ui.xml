<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
	<div style="padding-left:40%">
	<h4 class="text-light blue">View Registration</h4>
	
	<g:CheckBox ui:field="checkShowThisPanel">Show this panel</g:CheckBox>
	</div>
	<div style="padding-left:5px; padding-top:5px">
	<g:CheckBox ui:field="checkName">Name</g:CheckBox>
	</div>
	<div style="padding-left:5px; padding-top:5px">
	<g:CheckBox ui:field="checkEmail">Email</g:CheckBox>
	</div>
	<div style="padding-left:5px; padding-top:5px">
	<g:CheckBox ui:field="checkRegisteredTo">Registered to</g:CheckBox>
	</div>
	<div style="padding-left:5px; padding-top:5px">
	<g:CheckBox ui:field="checkUserName">User Name</g:CheckBox>
	</div>
	<div style="padding-left:5px; padding-top:5px">
	<g:CheckBox ui:field="checkLastTimeEdited">Last time edited</g:CheckBox>
	</div>
	<div style="padding-left:10px">
	<g:CheckBox ui:field="checkAllowEditing" visible="false">Allow Edidting</g:CheckBox>
	</div>
	
	</g:HTMLPanel>
</ui:UiBinder> 