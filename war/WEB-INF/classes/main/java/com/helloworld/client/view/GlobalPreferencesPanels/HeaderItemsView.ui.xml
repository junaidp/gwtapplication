<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v="urn:import:com.helloworld.client.view.widgets">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<div>
			<g:Label styleName="lblStyle inLine blue" text="Width: "></g:Label>
			<g:TextBox styleName="lblStyle inLine" ui:field="txtBannerWidth"></g:TextBox>
		</div>
		<div>
			<g:Label styleName="lblStyle inLine blue" text="Height: "></g:Label>
			<g:TextBox styleName="lblStyle inLine"  ui:field="txtBannerHeight"></g:TextBox>
		</div>
		<div>
			<g:Label styleName="lblStyle inLine blue" text="Logo: "></g:Label>
			<v:LogoAttachment ui:field="logoUrl"></v:LogoAttachment>
		</div>


		<g:Label styleName="blue">Menu</g:Label>
		<g:Tree ui:field="tree"></g:Tree>
		<g:Button ui:field="btnPreview">Preview</g:Button>
		<g:MenuBar ui:field="menuBar"></g:MenuBar>


	</g:HTMLPanel>
</ui:UiBinder> 