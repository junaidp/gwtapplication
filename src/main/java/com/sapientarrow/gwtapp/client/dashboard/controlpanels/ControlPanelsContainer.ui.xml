						<div class="metro-tile metro-tile-dashboard">
						<div id="content"></div>	
						Dashboard
						<C:DashboardPanelView ui:field="dashboardPanel"></C:DashboardPanelView>
						</div>
						</g:HTMLPanel>
						</g:SimplePanel>

					</td>
					<td>
					<g:SimplePanel ui:field="accordionContainer">
						<g:HTMLPanel>
						<div class="metro-tile metro-tile-small metro-tile-accordion">
						<div id="content"></div>	
							Accordion
							<C:DashboardAccordionPanelView
								ui:field="dashboardAccordionPanel"></C:DashboardAccordionPanelView>
						
						</div>
						</g:HTMLPanel>
						</g:SimplePanel>
					</td>

				</tr>
			</table>
			<table>
				<tr>
					<td>
					<g:SimplePanel ui:field="portalContainer">
						<g:HTMLPanel>
