<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:cellview="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
	<div style="padding-left:35%; padding-top:1%">
	<g:Button styleName="button primary" ui:field="btnAddBinding">Add Binding</g:Button>
	</div>
	<div style="padding-left:35%; padding:5px">
	
	<div style="width:100%">
					<cellview:CellTable width='100%' pageSize='20'
						ui:field='tableBindings' styleName="cellTable" />
					<cellview:SimplePager ui:field="pager"
						location='CENTER' addStyleNames="pagerPanel">
					</cellview:SimplePager>
				</div>
	
	
	<div style="padding-left:50%">
	 <g:Button ui:field="btnDeleteBindings" enabled="false"> Delete </g:Button>
	</div>
	</div>
	</g:HTMLPanel>
</ui:UiBinder> 