<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel>
			<g:Label text="Id" width="80px"/>
			<g:ListBox visibleItemCount="1" ui:field="listIds" />
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label text="Name" width="80px"/>
			<g:TextBox/>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label text="Password" width="81px"/>
			<g:PasswordTextBox width="162px"/>
		</g:HorizontalPanel>

	</g:HTMLPanel>
</ui:UiBinder> 