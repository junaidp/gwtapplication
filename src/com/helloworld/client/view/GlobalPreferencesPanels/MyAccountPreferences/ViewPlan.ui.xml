<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<div style="padding-left:40%">
			<h4 class="text-light blue">View Plan</h4>
			<g:CheckBox ui:field="checkShowThisPanel">Show this panel</g:CheckBox>
		</div>
		<div style="padding-left:5px; padding-top:5px">

			<g:CheckBox ui:field="checkBoxPaymentPlan" styleName="inLine"
				text="Payment Plan"></g:CheckBox>
		</div>
		<div style="padding-left:20px; padding-top:5px">
			<g:CheckBox ui:field="checkBoxMonthly" text="Monthly"></g:CheckBox>
		</div>
		<div style="padding-left:20px; padding-top:5px">
			<g:CheckBox ui:field="checkBoxQuaterly" text="Quartely"></g:CheckBox>
		</div>
		<div style="padding-left:20px; padding-top:5px">
			<g:CheckBox ui:field="checkBoxYearly" text="Yearly"></g:CheckBox>
		</div>



		<div style="padding:4px">
			<g:CheckBox ui:field="paymentDetails"> Payment Details</g:CheckBox>
		</div>

	</g:HTMLPanel>
</ui:UiBinder> 