<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<head>
			<link href="Metro-UI-CSS-master/build/css/metro.css" rel="stylesheet" />
		</head>
		<body class="metro">
			<div style="padding-left:40%">
				<div class="padding20">
					<h4 class="text-light blue">Manage User</h4>
					<form>

						<div>
							<g:Label ui:field="lblError" styleName="error"></g:Label>
						</div>
						<div>
						<g:ListBox ui:field="lisUsers" styleName="input-control select"></g:ListBox>
						</div>
						<div>
							<g:CheckBox styleName=""
								ui:field="checkActive">Active</g:CheckBox>
						</div>

						<div class="form-actions">
							<g:Button styleName="button primary" ui:field="btnSubmit">Save</g:Button>

						</div>

					</form>
				</div>

			</div>
		</body>

	</g:HTMLPanel>
</ui:UiBinder> 