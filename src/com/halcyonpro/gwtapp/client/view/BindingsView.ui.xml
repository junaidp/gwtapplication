<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client"
	xmlns:cellview="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<div style="padding-left:45%; padding-top:1%">
			<g:Button styleName="button primary" ui:field="btnAddBinding" width="100px">Add Binding</g:Button>
		</div>
		<div style="padding-left:28%">

		
			<g:DecoratorPanel>
				<g:VerticalPanel>
				<g:HTMLPanel>
					<g:TextBox ui:field="txtSearch"></g:TextBox>
					<g:VerticalPanel ui:field="cellContainer" width="600px">
					</g:VerticalPanel>
					
						<div style="padding-left:35%">
							<cellview:SimplePager ui:field="pager"
								location='CENTER' addStyleNames="pagerPanel">
							</cellview:SimplePager>
						</div>
						<div style="padding-left:87%">
							<g:Button ui:field="btnDeleteBindings" enabled="false" styleName="button danger" height="20px"> Delete </g:Button>
						</div>
					</g:HTMLPanel>
				</g:VerticalPanel>
			</g:DecoratorPanel>



		</div>
	</g:HTMLPanel>
</ui:UiBinder> 