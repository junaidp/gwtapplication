<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.rightAlignInline {
		text-align: right;
		style: "display: inline";
	}
	
	.bigFont {
		font-size: 130%;
	}
	</ui:style>

	<b.html:Div>
		<b:Container fluid="true" marginTop="45">
			<b:Row>
				<b:Column size="XS_12,SM_8,MD_9,LG_9">
					<b:Legend ui:field="header">Green Template</b:Legend>
				</b:Column>
				<b:Column size="XS_12,SM_4,MD_3,LG_3">
					
				</b:Column>
			</b:Row>
			<b:Row>

			</b:Row>
			<b:Row ui:field="mainRow">
				<b:Column size="XS_12,SM_6,MD_4,LG_4" marginTop="15">
					<b.html:Italic>some text:</b.html:Italic>
					<b.html:Br />
					<b.html:Div ui:field="realProperty"></b.html:Div>
				</b:Column>
				<b:Column size="XS_12,SM_6,MD_4,LG_4" marginTop="15">

				</b:Column>
			</b:Row>

			<b:Legend marginTop="40">Heading 2</b:Legend>
			<b.html:Div ui:field="leaseContracts"></b.html:Div>
			<b:Panel type="WARNING" ui:field="noLeaseContractsInfo">
				<b:PanelHeader>
					<b:Container fluid="true">
						<b.html:Span addStyleNames="{style.bigFont}">
							<b.html:Text>some text.</b.html:Text>
						</b.html:Span>
					</b:Container>
				</b:PanelHeader>
			</b:Panel>

		</b:Container>
	</b.html:Div>
</ui:UiBinder>
