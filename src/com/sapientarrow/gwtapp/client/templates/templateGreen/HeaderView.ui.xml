<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.heading {
		color: green;
	}
	</ui:style>
	<g:HTMLPanel>
		<b:Navbar>
			<b:NavbarHeader>
				<b:NavbarBrand styleName="heading">Corporate Green</b:NavbarBrand>
				<b:NavbarCollapseButton dataTarget="#navbar-collapse" />

				<b:NavbarForm pull="RIGHT">
					<b:TextBox placeholder="Search" addStyleNames="col-lg-8" />
				</b:NavbarForm>
			</b:NavbarHeader>

			<b:NavbarCollapse b:id="navbar-collapse">
				<b:NavbarNav pull="RIGHT">
					<b:AnchorListItem active="true">HOME</b:AnchorListItem>
					<b:AnchorListItem>SERVICE</b:AnchorListItem>
					<b:AnchorListItem>BLOG</b:AnchorListItem>
					<b:AnchorListItem>CONTACT</b:AnchorListItem>

				</b:NavbarNav>

			</b:NavbarCollapse>
		</b:Navbar>
	</g:HTMLPanel>
</ui:UiBinder> 