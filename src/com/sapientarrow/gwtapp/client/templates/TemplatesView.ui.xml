<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<b:Row>
			<b:Legend ui:field="typeStepMessage">Templates</b:Legend>
			<b:Column size="XS_12,SM_6,MD_6,LG_4">
				<g:Image ui:field="imgTemplate" url="images/dashboard.png"></g:Image>
				<g:Button styleName="button primary" width="120px" ui:field="btnSelect" text="Select"></g:Button>
				
			</b:Column>
		</b:Row>
	</g:HTMLPanel>
</ui:UiBinder> 