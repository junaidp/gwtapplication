<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<!-- <div class="browserRight"> -->
		<div style="padding-left:40%">
			<h4 class="text-light blue">Create/Update Account</h4>
			<g:HTMLPanel ui:field="panelName">
				<div style="padding:5px">
					<g:Label styleName="inLine" width="200px">Name</g:Label>
					<g:TextBox styleName="inLine textBoxStyle" ui:field="name"></g:TextBox>
					<g:Label styleName="inLine error" width="200px" ui:field="nameError"></g:Label>
				</div>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="panelEmail">
				<div style="padding:5px">
					<g:Label styleName="inLine" width="200px">Email</g:Label>
					<g:TextBox styleName="inLine textBoxStyle" ui:field="email"></g:TextBox>
					<g:Label styleName="inLine error" width="200px" ui:field="emailError"></g:Label>
				</div>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="panelUserName">
				<div style="padding:5px">
					<g:Label styleName="inLine" width="200px">Username</g:Label>
					<g:TextBox styleName="inLine textBoxStyle" ui:field="userName"></g:TextBox>
					<g:Label styleName="inLine error" width="200px" ui:field="userNameError"></g:Label>
				</div>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="panelRegisteredTo">
				<div style="padding:5px">
					<g:Label styleName="inLine" width="200px">Registered to </g:Label>
					<g:TextBox styleName="inLine textBoxStyle" ui:field="registeredTo"></g:TextBox>
					<g:Label styleName="inLine error" width="200px" ui:field="registedToError"></g:Label>
				</div>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="panelPassword">
				<div style="padding:5px">
					<g:Label styleName="inLine" width="200px">Password</g:Label>
					<g:PasswordTextBox styleName="inLine textBoxStyle"
						ui:field="password"></g:PasswordTextBox>
					<g:Label styleName="inLine error" width="200px" ui:field="passwordError"></g:Label>
				</div>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="panelConfirmPassword">
				<div style="padding:5px">
					<g:Label styleName="inLine" width="200px">Confirm Password</g:Label>
					<g:PasswordTextBox styleName="inLine textBoxStyle"
						ui:field="confirmPassword"></g:PasswordTextBox>
					<g:Label styleName="inLine error" width="200px"
						ui:field="confirmPasswordError"></g:Label>
				</div>
			</g:HTMLPanel>

			<div>
				<g:VerticalPanel ui:field="container"></g:VerticalPanel>
			</div>
			<div>
				<g:Label styleName="inLine error" width="200px" ui:field="captchaError"></g:Label>

			</div>

			<div style="padding-left:350px; padding-top:5px">

				<g:Button styleName="inLine  button primary" ui:field="btnSubmit" width="100px">
					Submit </g:Button>
			</div>

			<div style="padding-left:60%; padding-top:20px">
				<g:HTMLPanel ui:field="panelCloseAccount">
					<g:Button styleName="button danger" ui:field="btnCloseAccount"> Close
						Account </g:Button>
				</g:HTMLPanel>
			</div>

		</div>
	</g:HTMLPanel>
</ui:UiBinder> 